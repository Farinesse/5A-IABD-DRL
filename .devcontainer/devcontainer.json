{
  "name": "drl_space",
  "image": "mcr.microsoft.com/devcontainers/universal:2",
  "features": {
    "ghcr.io/devcontainers/features/desktop-lite:1": {
      "webPort": "8888"
    }
  },
  "forwardPorts": [8888],
  "portsAttributes": {
    "8888": {
      "label": "desktop"
    }
  },
  "runArgs": ["--shm-size=1g"]
}